.nav-custom{
  /* height: 100%; */
  display:block;
  position:relative;
  background-image:linear-gradient(45deg, #0081ff, #1cbbb4);
}
.titleName {
  position: absolute;
  left: 50%;
  transform: translate(-50%, -50%);
}
.defineSelfBTN {
  /* border: 1px solid #fff; */
  border-radius: 45px;
  padding: 6px 8px;
  box-sizing: border-box;
  background-color: rgba(43, 41, 41, 0.247);
  color: #fff;
  position: absolute;
}
.menuItem:first-child {
  border-right: 1px solid #fff;
  box-sizing: border-box;
}
.menuItem {
  width: 50%;
  height: 100%;
  box-sizing: border-box;
  padding: 0px 5px;
  text-align: center;
  float: left;
}
navigator {
  width: 100%;
  height: 100%;
}
image {
  width: 80%;
  height: 100%;
}
